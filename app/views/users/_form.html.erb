<%= render "layouts/errors", object: @user %>

<%= form_for @user, :html => {:class => "form"}  do |f| %>
  <%= f.label :username %>
  <%= f.text_field :username %>
  <%= f.label :email %>
  <%= f.text_field :email %>
  <%= f.label :password %>
  <%= f.password_field :password %>
  <%= f.submit :id => "submit"%>
<% end %>
